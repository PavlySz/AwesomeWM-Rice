{
  "editor.minimap.enabled": false,
  "workbench.editor.enablePreview": false,
  "editor.fontSize": 18,
  // "window.menuBarVisibility": "toggle", // alt to open Menu bar
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "terminal.integrated.inheritEnv": false,
  "jupyter.askForKernelRestart": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "files.autoSave": "afterDelay",
  // "editor.defaultFormatter": "ms-python.python",
  "editor.formatOnSave": true, // true
  "editor.formatOnType": true,
  "editor.formatOnPaste": true,
  "html.format.contentUnformatted": "",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 18,
  "terminal.integrated.lineHeight": 1,
  "editor.renderControlCharacters": true,
  "workbench.colorTheme": "Ayu Dark",
  // "python.defaultInterpreterPath": "/home/bot/anaconda3/bin/python",
  // "[python]": {
  //   "editor.defaultFormatter": "ms-python.vscode-pylance" // WAS ms-python.python
  // },
  "notebook.lineNumbers": "on",
  // "python.analysis.completeFunctionParens": true,
  // "python.analysis.typeCheckingMode": "basic",
  // "python.autoComplete.extraPaths": [
  //   "~/.local/lib/Google/google_appengine",
  //   "~/.local/lib/Google/google_appengine/lib/flask-0.12"
  // ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.analysis.extraPaths": [
    "~/.local/lib/Google/google_appengine",
    "~/.local/lib/Google/google_appengine/lib/flask-0.12"
  ],
  "python.languageServer": "Pylance",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  // "autoDocstring.quoteStyle": "'''",
  // Linting Py
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintArgs": [
    // "--disable=W1618", // C0103,W1618,W1632,W0141
    // "--extension-pkg-whitelist=cv2,numpy",
    // "--max-line-length=100"
    "--enable=F,E,W,C" //F,E,W
    // "--enable=F,E,W,unreachable,duplicate-key,unnecessary-semicolon,global-variable-not-assigned,unused-variable,binary-op-exception,bad-format-string,anomalous-backslash-in-string,bad-open-mode"
  ],
  "python.analysis.autoImportCompletions": true,
  "python.analysis.diagnosticMode": "openFilesOnly",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.typeCheckingMode": "basic",
  "vsintellicode.features.python.deepLearning": "enabled",
  "python.linting.pylintCategorySeverity.refactor": "Hint",
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--enable=E,W,F,C,N"],
  "python.linting.maxNumberOfProblems": 100,
  "python.linting.prospectorEnabled": true,
  "python.linting.prospectorArgs": ["--enable=E,W,F,C,N"],
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--max-line-length=100"],
  "python.linting.pycodestyleEnabled": true,
  "python.linting.pycodestyleCategorySeverity.W": "Hint",
  "python.linting.pycodestyleCategorySeverity.E": "Error",
  "python.linting.pycodestyleArgs": ["--max-line-length=100"],
  "security.workspace.trust.untrustedFiles": "open",
  // "kite.showWelcomeNotificationOnStartup": false,
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.symbolCacheDownload": true,
  "julia.enableTelemetry": false,
  "jupyter.interactiveWindowMode": "perFile",
  "notebook.output.textLineLimit": 120,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "razor": "html",
    "plaintext": "pug"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

